#!/bin/bash
# this file will be overwritten by the project setup script
SIMICS_BASE_PACKAGE="/home/marek/simics/simics-6.0.157"
export SIMICS_BASE_PACKAGE
if [ -f "/home/marek/simics-projects/my-simics-project-1/.package-list" ]; then
    exec "/home/marek/simics/simics-6.0.157/bin/simics-eclipse" --package-list "/home/marek/simics-projects/my-simics-project-1/.package-list" --project "/home/marek/simics-projects/my-simics-project-1" "$@"
else
    exec "/home/marek/simics/simics-6.0.157/bin/simics-eclipse" --project "/home/marek/simics-projects/my-simics-project-1" "$@"
fi
